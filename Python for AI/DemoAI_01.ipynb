    "data = [10, 20, 30, 40, 50]\n",
    "\n",
    "def result(data):\n",
    "     average = sum(data) / len(data)  \n",
    "     maximum = max(data)                 \n",
    "     minimum = min(data)   \n",
    "     return {\"Avg \": average, \"Max \": maximum, \"Min \": minimum}\n",
    "\n",
    "result(data)"
   ]
