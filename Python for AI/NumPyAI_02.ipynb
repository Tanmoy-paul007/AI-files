   "source": [
    "import numpy as np \n",
    "\n",
    "data = np.array([10, 20, 30, 40, 50])\n",
    "\n",
    "transfer = 2 * data + 1\n",
    "\n",
    "normalijation = (transfer - np.mean(transfer) / np.std(transfer))\n",
    "\n",
    "print(\"Original:\", data )\n",
    "print(\"Transformed:\", transfer)\n",
    "print(\"Normalized:\", normalijation)"
   ]
  }
